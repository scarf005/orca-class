; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Orca Class"
run/main_scene="res://scene/World.tscn"
run/low_processor_mode=true
boot_splash/image="res://assets/32x32.png"
boot_splash/bg_color=Color( 0.733333, 0.733333, 0.733333, 1 )
config/icon="res://assets/orca.png"

[debug]

gdscript/warnings/unused_class_variable=true
gdscript/warnings/integer_division=false
gdscript/warnings/unsafe_cast=true
gdscript/warnings/unsafe_call_argument=true

[display]

window/size/width=640
window/size/height=512
window/size/resizable=false
window/energy_saving/keep_screen_on=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.784314, 0.768627, 0.745098, 1 )
environment/default_environment="res://default_env.tres"
